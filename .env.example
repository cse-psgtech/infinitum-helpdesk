# Environment Variables
# Copy this file to .env.local and update with your values

# Backend API Base URL - Update with your backend server URL
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Admin API Key for backend authentication
NEXT_PUBLIC_ADMIN_API_KEY=your-admin-api-key-here

# Admin Login Credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your-secure-password-here
ADMIN_NAME=Admin

# Optional: Host IP for mobile scanner QR code (defaults to 10.155.34.158)
NEXT_PUBLIC_HOST_IP=10.155.34.158
